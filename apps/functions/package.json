{
  "name": "@gen-elevate/functions",
  "version": "1.0.0",
  "private": true,
  "description": "Firebase Cloud Functions for Gen Elevate",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf lib"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
